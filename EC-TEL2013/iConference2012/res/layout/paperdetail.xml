<LinearLayout android:id="@+id/RelativeLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:background="@drawable/White">
	<TextView android:id="@+id/TextView01"
		android:layout_height="wrap_content" 
		android:layout_width="fill_parent"
		android:textStyle="bold"
		android:textColor="@drawable/darkblue"
		android:textSize="8pt"
		android:padding="5pt">
	</TextView>
	
	<TextView android:id="@+id/TextView02" 
		android:layout_height="wrap_content" 
		android:layout_width="fill_parent" 
		android:paddingLeft="5pt"
		android:paddingRight="5pt"
		android:textColor="@drawable/Gray">
	</TextView>
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:orientation="horizontal"
		android:paddingLeft="5pt"
		android:paddingRight="5pt"
		android:paddingBottom="5pt">
	<TextView android:id="@+id/TextView03" 
		android:text="AT "
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:paddingRight="5pt"  
		android:textColor="@drawable/Gray">
	</TextView>
	<TextView android:id="@+id/TextView04" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:paddingRight="5pt"
		android:textColor="@drawable/SteelBlue">
	</TextView>
	</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:paddingLeft="5pt"
		android:paddingRight="5pt"
		android:orientation="horizontal">
	<TextView android:id="@+id/TextView05" 
		android:text="BY "
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:paddingRight="5pt"  
		android:textColor="@drawable/Gray">
	</TextView>
	<TextView android:id="@+id/TextView06" 
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:paddingRight="5pt"
		android:textColor="@drawable/SteelBlue">
	</TextView>
	</LinearLayout>
		<LinearLayout android:id="@+id/LinearLayout011"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:orientation="horizontal"
		android:paddingLeft="5pt"
		android:paddingRight="5pt"
		android:paddingBottom="5pt">
	<TextView android:id="@+id/TextView031" 
		android:text="PDF "
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:paddingRight="5pt"  
		android:textColor="@drawable/Gray">
	</TextView>
	<TextView 
	    android:id="@+id/PaperButton"
		android:layout_height="wrap_content" 
		android:layout_width="fill_parent"
		android:paddingRight="5pt"
		android:textColor="@drawable/SteelBlue">    
	</TextView>
	</LinearLayout>
	
	<View android:id="@+id/view1"   
    	android:layout_width="fill_parent"  
    	android:layout_height="2px"  
    	android:layout_below="@id/LinearLayout02"
    	android:background="?android:attr/listDivider"  
	/>  
	<LinearLayout android:id="@+id/LinearLayout03"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:paddingRight="5pt"
		android:paddingLeft="5pt"
		android:orientation="horizontal">
	<ImageButton android:id="@+id/ImageButton01"
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:src="@drawable/no_schedule"
        android:background="@drawable/White"
        android:paddingLeft="5pt">
    </ImageButton>
	<ImageButton android:id="@+id/ImageButton02"
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:src="@drawable/no_star"
        android:background="@drawable/White"
        android:paddingLeft="5pt">
    </ImageButton>
    <ImageButton android:id="@+id/ImageButton03"
		android:layout_height="wrap_content" 
		android:layout_width="wrap_content"
		android:src="@drawable/share"
        android:background="@drawable/White"
        android:paddingLeft="5pt">
    </ImageButton>
	</LinearLayout>
	<View android:id="@+id/view2"   
    	android:layout_width="fill_parent"  
    	android:layout_height="2px"  
    	android:background="?android:attr/listDivider"  
	/>  
	<WebView android:id="@+id/WebView01" 
		android:layout_height="fill_parent"
		android:layout_width="fill_parent" 
		android:padding="10pt">
	</WebView>
</LinearLayout>